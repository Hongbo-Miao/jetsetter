{
  "env": {
    "development": {
      "application/javascript": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "electron": "1.6"
              }
            }
          ],
          "react"
        ],
        "plugins": [
          "react-hot-loader/babel",
          "syntax-dynamic-import",
          "dynamic-import-node",
          "transform-async-to-generator",
          "transform-es2015-modules-commonjs",
          "transform-object-rest-spread"
        ],
        "sourceMaps": "inline"
      },
      "text/jade": {
        "pretty": true
      }
    },
    "production": {
      "application/javascript": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "electron": "1.6"
              }
            }
          ],
          "react"
        ],
        "plugins": [
          "syntax-dynamic-import",
          "dynamic-import-node",
          "transform-async-to-generator",
          "transform-es2015-modules-commonjs",
          "transform-object-rest-spread"
        ],
        "sourceMaps": "none"
      }
    }
  }
}
